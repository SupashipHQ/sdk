{
  "name": "@darkfeature/sdk-javascript",
  "version": "0.1.0",
  "description": "JavaScript SDK for DarkFeature",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "browser": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && tsc && tsc-alias",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.4",
    "tsc-alias": "^1.8.8",
    "typescript": "~5.3.3"
  },
  "keywords": [
    "feature-flags",
    "feature-toggles",
    "darkfeature"
  ],
  "author": "DarkFeature",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/darkfeature/sdk.git",
    "directory": "packages/javascript"
  },
  "bugs": {
    "url": "https://github.com/darkfeature/sdk/issues"
  },
  "homepage": "https://darkfeature.com"
}
