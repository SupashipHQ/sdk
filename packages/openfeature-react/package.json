{
  "name": "@supashiphq/openfeature-react-provider",
  "version": "0.1.0",
  "description": "OpenFeature React provider for Supaship feature flags",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "pnpm clean && pnpm build:tsup",
    "build:tsup": "tsup",
    "clean": "rimraf dist",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "openfeature",
    "react",
    "feature-flags",
    "feature-toggles",
    "supaship",
    "provider",
    "hooks"
  ],
  "author": "Supaship",
  "license": "MIT",
  "peerDependencies": {
    "@openfeature/react-sdk": "^1.0.0",
    "react": "^18.0.0"
  },
  "dependencies": {
    "@supashiphq/javascript-sdk": "workspace:*",
    "@supashiphq/react-sdk": "workspace:*",
    "@supashiphq/openfeature-js-provider": "workspace:*"
  },
  "devDependencies": {
    "@openfeature/react-sdk": "^1.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/supashiphq/sdk.git",
    "directory": "packages/openfeature-react"
  },
  "publishConfig": {
    "access": "public"
  }
}
