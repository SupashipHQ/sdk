{
  "name": "@darkfeature/sdk-react",
  "version": "0.1.0",
  "description": "DarkFeature SDK for React",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "browser": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && tsc && tsc-alias",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@darkfeature/sdk-javascript": "^0.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.0",
    "tsc-alias": "^1.8.8",
    "typescript": "~5.3.3"
  },
  "keywords": [
    "feature-flags",
    "feature-toggles",
    "react",
    "sdk"
  ],
  "author": "DarkFeature",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/darkfeature/sdk.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/darkfeature/sdk/issues"
  },
  "homepage": "https://github.com/darkfeature/sdk/tree/main/packages/react#readme"
}
